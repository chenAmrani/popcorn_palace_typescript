{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "./test/coverage",
  "testEnvironment": "node",
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.ts",
    "!src/main.ts",
    "!src/**/*.module.ts",
    "!src/**/entities/*.ts",
    "!src/**/dto/*.ts"
  ]
}
